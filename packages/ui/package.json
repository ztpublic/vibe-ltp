{
  "name": "@vibe-ltp/ui",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./chatbot": {
      "types": "./dist/chatbot/index.d.ts",
      "import": "./dist/chatbot/index.js",
      "require": "./dist/chatbot/index.js",
      "default": "./dist/chatbot/index.js"
    },
    "./chatbot/main.css": "./dist/chatbot/main.css",
    "./chatbot/*": "./dist/chatbot/*"
  },
  "scripts": {
    "build": "tsc --build && node ./scripts/copy-chatbot-assets.js",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "copy-chatbot-assets": "node ./scripts/copy-chatbot-assets.js"
  },
  "dependencies": {
    "@vibe-ltp/shared": "workspace:*",
    "react-conditionally-render": "^1.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vibe-ltp/config": "workspace:*",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.6.3"
  }
}
